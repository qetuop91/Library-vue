{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'AdminList',\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 14,\n        username: '',\n        phone: '',\n        email: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/admin/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === 200) {\n          console.log('res: ', res);\n          console.log('this.params: ', this.params);\n          console.log('total: ', res.data.total);\n          console.log('pageNum: ', res.data.current);\n          console.log('pageSize: ', res.data.size);\n          this.total = Number(res.data.total);\n          this.tableData = res.data.records;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 14,\n        username: '',\n        phone: '',\n        email: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击触发分页按钮\n      console.log(pageNum);\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete('/admin/delete/' + id).then(res => {\n        console.log(res);\n        if (res.code === 200) {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    resetpwd(id) {\n      request.post('/admin/resetpwd');\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","tableData","total","params","pageNum","pageSize","username","phone","email","created","load","methods","get","then","res","code","console","log","current","size","Number","records","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg","resetpwd","post"],"sources":["src/views/admin/AdminList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <!-- 搜索表单 -->\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入用户名\" v-model=\"params.username\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入联系方式\" v-model=\"params.phone\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入邮箱\" v-model=\"params.email\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"联系方式\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"updatetime\" label=\"修改时间\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n          <!-- scope.row就是当前行数据 -->\r\n          <el-button type=\"primary\" @click=\"$router.push('/editAdmin?id=' + scope.row.id)\">编辑</el-button>\r\n          <el-popconfirm style=\"margin-left: 5px\" title=\"确定删除这一数据?\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n          <el-popconfirm style=\"margin-left: 5px\" title=\"确定重置密码为123456吗?\" @confirm=\"resetpwd(scope.row.id)\">\r\n            <el-button type=\"warning\" slot=\"reference\">编辑</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next, total\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: 'AdminList',\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        username: '',\r\n        phone: '',\r\n        email: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/admin/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === 200) {\r\n          console.log('res: ', res)\r\n          console.log('this.params: ', this.params)\r\n          console.log('total: ', res.data.total)\r\n          console.log('pageNum: ', res.data.current)\r\n          console.log('pageSize: ', res.data.size)\r\n          this.total = Number(res.data.total)\r\n          this.tableData = res.data.records\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        username: '',\r\n        phone: '',\r\n        email: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击触发分页按钮\r\n      console.log(pageNum)\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(id) {\r\n      request.delete('/admin/delete/' + id).then(res => {\r\n        console.log(res)\r\n        if (res.code === 200) {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    resetpwd(id) {\r\n      request.post('/admin/resetpwd')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA8CA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACAZ,OAAA,CAAAc,GAAA;QACAT,MAAA,OAAAA;MACA,GAAAU,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACAC,OAAA,CAAAC,GAAA,UAAAH,GAAA;UACAE,OAAA,CAAAC,GAAA,uBAAAd,MAAA;UACAa,OAAA,CAAAC,GAAA,YAAAH,GAAA,CAAAd,IAAA,CAAAE,KAAA;UACAc,OAAA,CAAAC,GAAA,cAAAH,GAAA,CAAAd,IAAA,CAAAkB,OAAA;UACAF,OAAA,CAAAC,GAAA,eAAAH,GAAA,CAAAd,IAAA,CAAAmB,IAAA;UACA,KAAAjB,KAAA,GAAAkB,MAAA,CAAAN,GAAA,CAAAd,IAAA,CAAAE,KAAA;UACA,KAAAD,SAAA,GAAAa,GAAA,CAAAd,IAAA,CAAAqB,OAAA;QACA;MACA;IACA;IACAC,MAAA;MACA,KAAAnB,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACA,KAAAE,IAAA;IACA;IACAa,oBAAAnB,OAAA;MACA;MACAY,OAAA,CAAAC,GAAA,CAAAb,OAAA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAM,IAAA;IACA;IACAc,IAAAC,EAAA;MACA3B,OAAA,CAAA4B,MAAA,oBAAAD,EAAA,EAAAZ,IAAA,CAAAC,GAAA;QACAE,OAAA,CAAAC,GAAA,CAAAH,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAY,OAAA,CAAAC,OAAA;UACA,KAAAlB,IAAA;QACA;UACA,KAAAiB,OAAA,CAAAE,KAAA,CAAAf,GAAA,CAAAgB,GAAA;QACA;MACA;IACA;IACAC,SAAAN,EAAA;MACA3B,OAAA,CAAAkC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}