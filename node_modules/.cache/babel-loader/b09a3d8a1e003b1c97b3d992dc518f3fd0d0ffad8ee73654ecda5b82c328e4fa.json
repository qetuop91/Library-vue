{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: 'CategoryList',\n  data() {\n    return {\n      // category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\n      tableData: [],\n      total: 0,\n      dialogFormVisible: false,\n      form: {},\n      pid: null,\n      params: {\n        pageNum: 1,\n        pageSize: 14,\n        categoryName: ''\n      },\n      rules: {\n        categoryName: [{\n          required: true,\n          message: '请输入分类名',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/category/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === 200) {\n          console.log(res);\n          this.total = Number(res.data.total);\n          console.log(this.total);\n          this.tableData = res.data;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 14,\n        categoryName: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击触发分页按钮\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete('/category/delete/' + id).then(res => {\n        if (res.code === 200) {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    handleAdd(row) {\n      //将当前行的id作为二级分类的pid\n      this.pid = row.id;\n      this.dialogFormVisible = true;\n    },\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          this.form.pid = this.pid;\n          request.post('/category/save', this.form).then(res => {\n            if (res.code === 200) {\n              this.$notify.success('新增子分类成功');\n              this.$refs['ruleForm'].resetFields();\n              this.dialogFormVisible = false;\n              this.load();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","tableData","total","dialogFormVisible","form","pid","params","pageNum","pageSize","categoryName","rules","required","message","trigger","created","load","methods","get","then","res","code","console","log","Number","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg","handleAdd","row","save","$refs","validate","valid","post","resetFields"],"sources":["src/views/category/CategoryList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <!-- 搜索表单 -->\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入分类名称\" v-model=\"params.categoryName\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe row-key=\"id\" default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"categoryName\" label=\"分类名\"></el-table-column>\r\n      <el-table-column prop=\"remark\" label=\"备注\"></el-table-column>\r\n      <el-table-column prop=\"createTime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"updateTime\" label=\"修改时间\"></el-table-column>\r\n\r\n      <!-- 操作栏 -->\r\n      <el-table-column label=\"操作\" width=\"250px\" style=\"text-align: center\">\r\n        <!-- scope.row就是当前行数据 -->\r\n        <template v-slot=\"scope\">\r\n          <!-- 编辑 -->\r\n          <el-button type=\"success\" v-if=\"!scope.row.pid\" @click=\"handleAdd(scope.row)\">添加子分类</el-button>\r\n          <el-button type=\"primary\" @click=\"$router.push('/editCategory?id=' + scope.row.id)\">编辑</el-button>\r\n          <!-- 删除 -->\r\n          <el-popconfirm style=\"margin-left: 5px\" title=\"确定删除这一数据?\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next, total\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"添加子分类\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n      <el-form :model=\"form\" label-width=\"100px\" ref=\"ruleForm\" :rules=\"rules\" style=\"width: 85%\">\r\n        <el-form-item label=\"分类名\" prop=\"categoryName\">\r\n          <el-input v-model=\"form.categoryName\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"备注\" prop=\"remark\">\r\n          <el-input v-model=\"form.remark\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n  name: 'CategoryList',\r\n  data() {\r\n    return {\r\n      // category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      dialogFormVisible: false,\r\n      form: {},\r\n      pid: null,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        categoryName: ''\r\n      },\r\n      rules: {\r\n        categoryName: [\r\n          {required: true, message: '请输入分类名', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/category/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === 200) {\r\n          console.log(res)\r\n          this.total = Number(res.data.total)\r\n          console.log(this.total)\r\n          this.tableData = res.data\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        categoryName: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击触发分页按钮\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(id) {\r\n      request.delete('/category/delete/' + id).then(res => {\r\n        if (res.code === 200) {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleAdd(row) {\r\n      //将当前行的id作为二级分类的pid\r\n      this.pid = row.id\r\n      this.dialogFormVisible = true\r\n    },\r\n    save() {\r\n      this.$refs['ruleForm'].validate((valid) => {\r\n        if (valid) {\r\n          this.form.pid = this.pid\r\n          request.post('/category/save', this.form).then(res => {\r\n            if (res.code === 200) {\r\n              this.$notify.success('新增子分类成功');\r\n              this.$refs['ruleForm'].resetFields();\r\n              this.dialogFormVisible = false;\r\n              this.load()\r\n            } else {\r\n              this.$notify.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA4DA,OAAAA,OAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACA;