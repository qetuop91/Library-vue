{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: 'CategoryList',\n  data() {\n    return {\n      // category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 14,\n        categoryName: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/category/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === 200) {\n          this.total = Number(res.data.total);\n          this.tableData = res.data.records;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 14,\n        categoryName: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击触发分页按钮\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete('/category/delete/' + id).then(res => {\n        if (res.code === 200) {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","tableData","total","params","pageNum","pageSize","categoryName","created","load","methods","get","then","res","code","Number","records","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg"],"sources":["src/views/category/CategoryList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <!-- 搜索表单 -->\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入分类名称\" v-model=\"params.categoryName\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe row-key=\"id\" default-expand-all=\"true\">\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"categoryName\" label=\"分类名\"></el-table-column>\r\n      <el-table-column prop=\"remark\" label=\"备注\"></el-table-column>\r\n      <el-table-column prop=\"createTime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"updateTime\" label=\"修改时间\"></el-table-column>\r\n\r\n      <!-- 操作栏 -->\r\n      <el-table-column label=\"操作\" width=\"250px\" style=\"text-align: center\">\r\n        <!-- scope.row就是当前行数据 -->\r\n        <template v-slot=\"scope\">\r\n          <!-- 编辑 -->\r\n          <el-button type=\"primary\" @click=\"$router.push('/editCategory?id=' + scope.row.id)\">编辑</el-button>\r\n          <!-- 删除 -->\r\n          <el-popconfirm style=\"margin-left: 5px\" title=\"确定删除这一数据?\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next, total\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n  name: 'CategoryList',\r\n  data() {\r\n    return {\r\n      // category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        categoryName: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/category/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === 200) {\r\n          this.total = Number(res.data.total)\r\n          this.tableData = res.data.records\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        categoryName: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击触发分页按钮\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(id) {\r\n      request.delete('/category/delete/' + id).then(res => {\r\n        if (res.code === 200) {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA4CA,OAAAA,OAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,YAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACAX,OAAA,CAAAa,GAAA;QACAP,MAAA,OAAAA;MACA,GAAAQ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAX,KAAA,GAAAY,MAAA,CAAAF,GAAA,CAAAZ,IAAA,CAAAE,KAAA;UACA,KAAAD,SAAA,GAAAW,GAAA,CAAAZ,IAAA,CAAAe,OAAA;QACA;MACA;IACA;IACAC,MAAA;MACA,KAAAb,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,YAAA;MACA;MACA,KAAAE,IAAA;IACA;IACAS,oBAAAb,OAAA;MACA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAI,IAAA;IACA;IACAU,IAAAC,EAAA;MACAtB,OAAA,CAAAuB,MAAA,uBAAAD,EAAA,EAAAR,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAQ,OAAA,CAAAC,OAAA;UACA,KAAAd,IAAA;QACA;UACA,KAAAa,OAAA,CAAAE,KAAA,CAAAX,GAAA,CAAAY,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}