l-table-column prop=\"userName\" label=\"用户名称\"></el-table-column>\r\n      <el-table-column prop=\"userUsername\" label=\"用户卡号\"></el-table-column>\r\n      <el-table-column prop=\"userPhone\" label=\"联系方式\"></el-table-column>\r\n      <el-table-column prop=\"score\" label=\"书籍积分\"></el-table-column>\r\n      <el-table-column prop=\"createTime\" label=\"创建时间\"></el-table-column>\r\n<!--      <el-table-column prop=\"updateTime\" label=\"修改时间\"></el-table-column>-->\r\n      <el-table-column prop=\"status\" label=\"借出状态\"></el-table-column>\r\n      <el-table-column prop=\"days\" label=\"借出天数\"></el-table-column>\r\n      <el-table-column prop=\"returnDate\" label=\"归还日期\"></el-table-column>\r\n      <!-- 操作栏 -->\r\n      <el-table-column label=\"操作\" width=\"140px\"  style=\"text-align: center\">\r\n        <!-- scope.row就是当前行数据 -->\r\n        <template v-slot=\"scope\">\r\n          <!-- 编辑 -->\r\n<!--          <el-button type=\"primary\" @click=\"$router.push('/editBorrow?id=' + scope.row.id)\">编辑</el-button>-->\r\n          <!-- 还书 -->\r\n          <el-popconfirm title=\"确定还书?\" @confirm=\"returnBook(scope.row.id)\">\r\n            <el-button type=\"warning\" slot=\"reference\">还书</el-button>\r\n          </el-popconfirm>\r\n          <!-- 删除 -->\r\n          <el-popconfirm title=\"确定删除这一数据?\"style=\"margin-left: 5px\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next, total\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/u