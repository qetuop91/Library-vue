{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: 'BookList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 14,\n        bookName: '',\n        bookNumber: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/book/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === 200) {\n          console.log(res);\n          this.total = Number(res.data.total);\n          this.tableData = res.data.records;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 14,\n        bookName: '',\n        bookNumber: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击触发分页按钮\n      console.log(pageNum);\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete('/book/delete/' + id).then(res => {\n        if (res.code === 200) {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","admin","get","JSON","parse","tableData","total","params","pageNum","pageSize","bookName","bookNumber","created","load","methods","then","res","code","console","log","Number","records","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg"],"sources":["src/views/book/BookList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <!-- 搜索表单 -->\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入书籍名称\" v-model=\"params.bookName\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入书籍标准码\" v-model=\"params.bookNumber\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe row-key=\"id\" style=\"height: 100px\" default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"bookName\" label=\"书籍名称\"></el-table-column>\r\n      <el-table-column prop=\"description\" label=\"备注\"></el-table-column>\r\n      <el-table-column prop=\"publishDate\" label=\"出版日期\"></el-table-column>\r\n      <el-table-column prop=\"author\" label=\"作者\"></el-table-column>\r\n      <el-table-column prop=\"publish\" label=\"出版社\"></el-table-column>\r\n      <el-table-column prop=\"category\" label=\"分类\"></el-table-column>\r\n      <el-table-column prop=\"bookNumber\" label=\"书籍标准码\"></el-table-column>\r\n      <el-table-column prop=\"cover\" label=\"封面\">\r\n        <template v-slot=\"scope\">\r\n          <el-image :src=\"scope.row.cover\" :preview-src-list=\"[scope.row.cover]\"></el-image>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"createTime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"updateTime\" label=\"修改时间\"></el-table-column>\r\n\r\n      <!-- 操作栏 -->\r\n      <el-table-column label=\"操作\" width=\"250px\" style=\"text-align: center\">\r\n        <!-- scope.row就是当前行数据 -->\r\n        <template v-slot=\"scope\">\r\n          <!-- 编辑 -->\r\n          <el-button type=\"primary\" @click=\"$router.push('/editBook?id=' + scope.row.id)\">编辑</el-button>\r\n          <!-- 删除 -->\r\n          <el-popconfirm style=\"margin-left: 5px\" title=\"确定删除这一数据?\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next, total\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n  name: 'BookList',\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        bookName: '',\r\n        bookNumber: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/book/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === 200) {\r\n          console.log(res)\r\n          this.total = Number(res.data.total)\r\n          this.tableData = res.data.records\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        bookName: '',\r\n        bookNumber: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击触发分页按钮\r\n      console.log(pageNum)\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(id) {\r\n      request.delete('/book/delete/' + id).then(res => {\r\n        if (res.code === 200) {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAuDA,OAAAA,OAAA;AACA,OAAAC,OAAA;AAEA