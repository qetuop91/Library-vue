{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'AddBorrow',\n  data() {\n    return {\n      form: {\n        score: 0,\n        nums: 0,\n        bookName: '',\n        bookNumber: ''\n      },\n      books: [],\n      users: [],\n      rules: {\n        bookName: [{\n          required: true,\n          message: '请输入书籍名称',\n          trigger: 'blur'\n        }],\n        bookNumber: [{\n          required: true,\n          message: '请输入书籍标准码',\n          trigger: 'blur'\n        }],\n        score: [{\n          required: true,\n          message: '请输入借书积分',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    request.get('/book/list').then(res => {\n      console.log(res);\n      this.books = res.data;\n    });\n    request.get('/user/list').then(res => {\n      console.log(res);\n      this.users = res.data;\n    });\n  },\n  methods: {\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          request.post('/borrow/save', this.form).then(res => {\n            console.log(this.form);\n            if (res.code === 200) {\n              this.$notify.success('新增成功');\n              this.$refs['ruleForm'].resetFields();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    selectBookNumber() {\n      // console.log(this.books)\n      const book = this.books.find(v => v.bookNumber === this.form.bookNumber);\n      // console.log(book)\n      request.get('/book/' + book.id).then(res => {\n        // console.log(res.data)\n        console.log(this.form.score);\n        this.form.score = res.data.score;\n        this.form.nums = res.data.nums;\n        this.form.bookName = res.data.bookName;\n      });\n    },\n    selectBookName() {\n      const book = this.books.find(v => v.bookName === this.form.bookName);\n      request.get('/book/' + book.id).then(res => {\n        console.log(res);\