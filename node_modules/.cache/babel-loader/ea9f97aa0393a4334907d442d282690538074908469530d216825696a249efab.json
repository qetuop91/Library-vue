</el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"联系方式\" prop=\"userPhone\">\r\n        <el-input v-model=\"form.userPhone\" disabled></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"用户账户积分\" prop=\"account\">\r\n        <el-input v-model=\"form.account\" disabled></el-input>\r\n      </el-form-item>\r\n\r\n      <div style=\"text-align: center; margin-top: 30px\">\r\n        <el-button type=\"primary\" @click=\"save\" size=\"medium\">提交</el-button>\r\n      </div>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: 'AddBorrow',\r\n  data() {\r\n    return {\r\n      form: {},\r\n      books: [],\r\n      users: [],\r\n      rules: {\r\n        bookNumber: [\r\n          {required: true, message: '请输入书籍标准码', trigger: 'blur'}\r\n        ],\r\n        userUsername: [\r\n          {required: true, message: '请输入用户卡号', trigger: 'blur'}\r\n        ],\r\n\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    request.get('/book/list').then(res => {\r\n      console.log('/book/list: ', res)\r\n      this.books = res.data\r\n    })\r\n\r\n    request.get('/user/list').then(res => {\r\n      console.log('/user/list: ', res)\r\n      this.users = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    save() {\r\n      this.$refs['ruleForm'].validate((valid) => {\r\n        if (valid) {\r\n          console.log(this.form)\r\n          request.post('/borrow/save', this.form).then(res => {\r\n            if (res.code === 200) {\r\n              this.$notify.success('新增成功');\r\n              this.$refs['ruleForm'].resetFields();\r\n            } else {\r\n              this.$notify.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    selectBookNumber() {\r\n      const book = this.books.find(v => v.bookNumber === this.form.bookNumber)\r\n      request.get('/book/' + book.id).then(res => {\r\n        this.form.bookName = res.data.bookName\r\n        this.form.score = res.data.score\r\n        this.form.nums = res.data.nums\r\n      })\r\n    },\r\n    selectUserCard() {\r\n      const user = this.users.find(v => v.username === this.form.userUsername)\r\n      request.get('/user/' + user.id).then(res => {\r\n        this.form.userName = res.data.name\r\n        this.form.userPhone = res.data.phone\r\n        this.form.account = Number(res.data.account)\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n</style>"],"mappings":"AAsDA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,KAAA;MACAC,KAAA;QACAC,UAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,YAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAGA;IACA;EACA;EACAE,QAAA;IACAZ,OAAA,CAAAa,GAAA,eAAAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,iBAAAF,GAAA;MACA,KAAAX,KAAA,GAAAW,GAAA,CAAAb,IAAA;IACA;IAEAF,OAAA,CAAAa,GAAA,eAAAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,iBAAAF,GAAA;MACA,KAAAV,KAAA,GAAAU,GAAA,CAAAb,IAAA;IACA;EACA;EACAgB,OAAA;IACAC,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAN,OAAA,CAAAC,GAAA,MAAAd,IAAA;UACAH,OAAA,CAAAuB,IAAA,sBAAApB,IAAA,EAAAW,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAS,IAAA;cACA,KAAAC,OAAA,CAAAC,OAAA;cACA,KAAAN,KAAA,aAAAO,WAAA;YACA;cACA,KAAAF,OAAA,CAAAG,KAAA,CAAAb,GAAA,CAAAc,GAAA;YACA;UACA;QACA;MACA;IACA;IACAC,iBAAA;MACA,MAAAC,IAAA,QAAA3B,KAAA,CAAA4B,IAAA,CAAAC,CAAA,IAAAA,CAAA,CAAA1B,UAAA,UAAAJ,IAAA,CAAAI,UAAA;MACAP,OAAA,CAAAa,GAAA,YAAAkB,IAAA,CAAAG,EAAA,EAAApB,IAAA,CAAAC,GAAA;QACA,KAAAZ,IAAA,CAAAgC,QAAA,GAAApB,GAAA,CAAAb,IAAA,CAAAiC,QAAA;QACA,KAAAhC,IAAA,CAAAiC,KAAA,GAAArB,GAAA,CAAAb,IAAA,CAAAkC,KAAA;QACA,KAAAjC,IAAA,CAAAkC,IAAA,GAAAtB,GAAA,CAAAb,IAAA,CAAAmC,IAAA;MACA;IACA;IACAC,eAAA;MACA,MAAAC,IAAA,QAAAlC,KAAA,CAAA2B,IAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAO,QAAA,UAAArC,IAAA,CAAAQ,YAAA;MACAX,OAAA,CAAAa,GAAA,YAAA0B,IAAA,CAAAL,EAAA,EAAApB,IAAA,CAAAC,GAAA;QACA,KAAAZ,IAAA,CAAAsC,QAAA,GAAA1B,GAAA,CAAAb,IAAA,CAAAD,IAAA;QACA,KAAAE,IAAA,CAAAuC,SAAA,GAAA3B,GAAA,CAAAb,IAAA