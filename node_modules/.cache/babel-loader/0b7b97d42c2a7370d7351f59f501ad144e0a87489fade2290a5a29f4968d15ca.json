{"ast":null,"code":"import request from \"@/utils/request\";\nimport { getToken } from \"@/utils/auth\";\nexport default {\n  name: 'UserList',\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 14,\n        name: '',\n        phone: ''\n      },\n      dialogFormVisible: false,\n      form: {},\n      rules: {\n        score: [{\n          require: true,\n          message: '请输入积分',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/user/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === 200) {\n          this.total = Number(res.data.total);\n          this.tableData = res.data.records;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 14,\n        name: '',\n        phone: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击触发分页按钮\n      console.log(pageNum);\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete('/user/delete/' + id).then(res => {\n        console.log(res);\n        if (res.code === 200) {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    handleAccountAdd(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n    addAccount() {\n      request.post('/user/account', this.form).then(res => {\n        if (res.code === 200) {\n          console.log(res);\n          this.dialogFormVisible = false;\n          this.$notify.success('充值成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","getToken","name","data","tableData","total","params","pageNum","pageSize","phone","dialogFormVisible","form","rules","score","require","message","trigger","created","load","methods","get","then","res","code","Number","records","reset","handleCurrentChange","console","log","del","id","delete","$notify","success","error","msg","handleAccountAdd","row","JSON","parse","stringify","addAccount","post"],"sources":["src/views/user/UserList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <!-- 搜索表单 -->\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入名称\" v-model=\"params.name\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入联系方式\" v-model=\"params.phone\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"卡号\"></el-table-column>\r\n      <el-table-column prop=\"account\" label=\"积分余额\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\r\n      <el-table-column prop=\"age\" label=\"年龄\"></el-table-column>\r\n      <el-table-column prop=\"sex\" label=\"性别\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话号码\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\r\n      <el-table-column prop=\"createTime\" label=\"创建时间\"></el-table-column>\r\n      <el-table-column prop=\"updateTime\" label=\"修改时间\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"210\">\r\n        <template v-slot=\"scope\">\r\n          <!-- scope.row就是当前行数据 -->\r\n          <el-button type=\"warning\" @click=\"handleAccountAdd(scope.row)\">充值</el-button>\r\n          <el-button type=\"primary\" @click=\"$router.push('/editUser?id=' + scope.row.id)\">编辑</el-button>\r\n\r\n          <el-popconfirm style=\"margin-left: 5px\" title=\"确定删除这一数据?\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next, total\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"充值\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n      <el-form :model=\"form\" label-width=\"100px\" ref=\"ruleForm\" :rules=\"rules\" style=\"width: 85%\">\r\n        <el-form-item label=\"当前账户积分\" prop=\"account\">\r\n          <el-input disabled v-model=\"form.account\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"积分\" prop=\"score\">\r\n          <el-input v-model=\"form.score\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addAccount\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport {getToken} from \"@/utils/auth\";\r\n\r\nexport default {\r\n  name: 'UserList',\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        name: '',\r\n        phone: ''\r\n      },\r\n      dialogFormVisible: false,\r\n      form: {},\r\n      rules: {\r\n        score:[\r\n          {require: true, message: '请输入积分', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/user/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === 200) {\r\n          this.total = Number(res.data.total)\r\n          this.tableData = res.data.records\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 14,\r\n        name: '',\r\n        phone: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击触发分页按钮\r\n      console.log(pageNum)\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(id) {\r\n      request.delete('/user/delete/' + id).then(res => {\r\n        console.log(res)\r\n        if (res.code === 200) {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleAccountAdd(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n    },\r\n    addAccount() {\r\n      request.post('/user/account', this.form).then(res => {\r\n        if (res.code === 200) {\r\n          console.log(res)\r\n          this.dialogFormVisible = false\r\n          this.$notify.success('充值成功')\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA+DA,OAAAA,OAAA;AACA,SAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAN,IAAA;QACAO,KAAA;MACA;MACAC,iBAAA;MACAC,IAAA;MACAC,KAAA;QACAC,KAAA,GACA;UAAAC,OAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACAlB,OAAA,CAAAoB,GAAA;QACAd,MA